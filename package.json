{
	"name": "yak-cmd",
	"version": "1.0.0",
	"description": "some cmd tools",
	"main": "index.js",
	"type": "commonjs",
	"scripts": {
		"write": "node ./scripts/update-readme/index.js && pnpm prettier --write README.md",
		"prepare": "husky",
		"prettier": "prettier --write .",
		"dev": "tsup ./src/index.ts --config tsup.config.ts --watch",
		"build": "tsup ./src/index.ts --config tsup.config.ts"
	},
	"bin": {
		"yak": "./index.js"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@commitlint/cli": "19.8.0",
		"@commitlint/config-conventional": "19.8.0",
		"@eslint/js": "9.22.0",
		"@types/node": "22.13.10",
		"commander": "13.1.0",
		"cz-git": "1.11.1",
		"eslint": "9.22.0",
		"globals": "16.0.0",
		"husky": "9.1.7",
		"lint-staged": "15.5.0",
		"prettier": "3.5.3",
		"tsup": "8.4.0",
		"tsx": "4.19.3",
		"typescript": "5.8.2",
		"typescript-eslint": "8.27.0"
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.md": [
			"prettier --write --parser markdown --prose-wrap never"
		]
	}
}
